import greenfoot.*;
import java.awt.*;
public class Margin extends Actor{

    public Margin(int x, int y){
        Image(x,y);   
    }

    public void Image(int x, int y){
        GreenfootImage aimage = new GreenfootImage(x, y);
        aimage.setColor(Color.black);
        aimage.fill();
        setImage(aimage);
    }
}
